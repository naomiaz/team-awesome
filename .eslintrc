{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "jest": true,
    "node": true
  },
  "parser": "babel-eslint",
  "plugins": ["babel"],
  "root": true,
  "rules": {
    "comma-dangle": [ "error", "never" ],
    "jsx-a11y/label-has-for": "off",
    "jsx-a11y/label-has-associated-control": "off",
    "arrow-parens": ["error", "always"]
  }
}
